		, Run CommandReader "rathty" "rathty"
		, Run CommandReader "bash -c 'while true; do amixer -M -c 0 get Master | tail -n 1 | grep -o [0-9]*% | xargs -n1 anybar -t â™« -l 8; sleep 0.5; done'" "volume"
    ]
  , sepChar = "%"
  , alignSep = "}{"
